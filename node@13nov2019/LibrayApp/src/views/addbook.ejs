  <title>E-Library</title>
</head>
<link rel="stylesheet" href="/css/bootstrap.min.css">
<link rel="stylesheet" href="/css/mystyle.css">
<script src="js/jquery.min.js"></script>
<script src="js/bootstrap.min.js"></script>

<body style="background-color: rgb(196, 196, 196);">
  <nav class="navbar navbar-default nav-custom li-text  bottom-marg">
    <div class=" li-text">
      <ul class="nav navbar-nav li-text">
        <% for(var i=0;i<nav.length;i++){%>
          <li><a href="<%=nav[i].link%>" class="li-text"><%=nav[i].tittle%></a></li>
          <%}%>
      </ul>
      <ul class="nav navbar-nav navbar-right">
        <li><a href="sign-up.html" class="li-text">Sign up</a></li>
        <li><a href="login.html" class="li-text">Login</a></li>
      </ul>
    </div>
  </nav>
  <tr>
    <div class="head-under">
      <h1 class="lib-textbook">
        <%=tittle%>
      </h1>
      <div class="emphty"></div>
    </div>
    <div class="bookcontiner" id="card-cont">
      <form action="/books/save" method="POST" name="addbookform">
      
        <div class="form-group">
          <label for="bookname" class="formlab">Book name</label>
          <input type="text" class="form-control" id="bookname" placeholder="bookname" name="bookname" id="bookname" required>
        </div>
        <br>
        <div class="form-group">
          <label for="author" class="formlab">Author</label>
          <input type="text" class="form-control" id="author" placeholder="author" name="author" id="author" required>
        </div>
        <br>
        <div class="form-group">
          <label for="genere" class="formlab">Genere</label>
          <input type="text" class="form-control" id="genere" placeholder="genere" name="genere" id="genere" required>
        </div>
        <div class="form-group">
          <label for="username" class="formlab">Image</label>
          <input type="text" class="form-control" id="image" placeholder="image" name="image" id="image" required>
        </div>
        <br>
        <br>
        <button type="submit" class="btn btn-secondary btn-lg btn-block buttonwidth" name="login" id="login">Add</button>
        <br>
        <br>
      </form>
    </div>

    </div>

</body>

</html>